{
  "name": "pb-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "deploy": "yarn build && gcloud app deploy",
    "dev": "babel-watch ./src/bin/www",
    "prod": "NODE_ENV=production npm run build && npm run start",
    "server": "babel-node ./src/bin/www",
    "start": "node ./dist/bin/www"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "firebase-admin": "^8.8.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "pg": "^7.14.0",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "babel-watch": "^7.0.0"
  }
}
